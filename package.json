{
  "name": "smart-select",
  "version": "1.0.0",
  "description": "Smart select is replacing and beautifying standard selects while keeping them async",
  "main": "src/js/smart-select.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "gulp",
    "test": "karma start karma.conf.js",
    "lint": "eslint src/js/smart-select.js"
  },
  "eslintConfig": {
    "extends": "airbnb-base/legacy",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "no-underscore-dangle": 0,
      "func-names": 0
    },
    "globals": {
      "define": false,
      "$": false
    }
  },
  "dependencies": {
    "gulp-header": "^1.7.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-connect": "^2.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-json-editor": "^2.2.1",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.4.0",
    "jasmine-core": "^2.3.4",
    "jquery": "^1.11.3",
    "karma": "^0.13.10",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-safari-launcher": "^0.1.1",
    "karma-virtualbox-launcher": "0.0.2",
    "normalize.css": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://davidecantoni@github.com/davidecantoni/smart-select.git"
  },
  "keywords": [
    "select",
    "multi-select",
    "standalone",
    "jquery",
    "library",
    "micro",
    "smart",
    "multi select"
  ],
  "author": "Davide Cantoni <dev@davidecantoni.ch>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidecantoni/smart-select/issues"
  },
  "homepage": "https://github.com/davidecantoni/smart-select#readme"
}
